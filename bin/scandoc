#!/usr/bin/env ruby
require 'open-uri'
require 'nokogiri'
require 'launchy'
require 'clipboard'
require 'colorize'
require 'pry'


require_relative "../lib/command_line_interface.rb"
require_relative "../lib/documentation_data.rb"
require_relative "../lib/scraper.rb"

user_input = ARGV[0]

if user_input == nil
  puts "Please type keyword:"
  user_input = gets.chomp
end

CommandLineInterface.new(user_input).run
